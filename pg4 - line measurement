#include <stdio.h>
#include <math.h>
int main(){
    int x1,x2,y1,y2;
    float res;
    printf("Enter the values (x1, y1) :");
    scanf("%d%d",&x1,&y1);
    printf("Enter the values (x2, y2) :");
    scanf("%d%d",&x2,&y2);
    res = sqrt(pow((x2-x1),2)+pow((y2-y1),2));
    printf("The Length B/w Two points is %.2f",res);
    return 0;
}
